<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field"
	default-cascade="save-update">
	<class name="ar.edu.unq.desapp.groupb.model.Diagnostic">
		<id column="ID" name="id">
			<generator class="native" />
		</id>

		<property name="name" />

		<!--<list name="symptoms" lazy="true">
			<key column="diagnosticID" />
			<list-index column="id" />
			<many-to-many column="symptomID" class="Symptom" />
		</list>-->
		
		<list name="symptoms" lazy="false">
			<key column="symptomID" />
			<list-index column="id" />
			<many-to-many class="ar.edu.unq.desapp.groupb.model.Symptom" />
		</list>
		
		<property name="treatment" />

	</class>
</hibernate-mapping>
