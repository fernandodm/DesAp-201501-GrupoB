<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field"
	default-cascade="save-update">
	<class name="ar.edu.unq.desapp.groupb.model.Event">
		<id column="ID" name="id">
			<generator class="native" />
		</id>

		<property name="date" type="org.joda.time.contrib.hibernate.PersistentDateTime"/>
		<!--<property name="diagnostic" lazy="false"/>-->
		
		<many-to-one name="diagnostic" class="ar.edu.unq.desapp.groupb.model.Diagnostic"
			cascade="save-update" lazy="false"></many-to-one>

	</class>
</hibernate-mapping>
