<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field"
	default-cascade="save-update">
	<class name="ar.edu.unq.desapp.groupb.model.Event" lazy="false">
		<id column="ID" name="id">
			<generator class="native" />
		</id>

		<property name="date" />
		<property name="diagnostic" lazy="false"/>
		
		<!--<one-to-one name="diagnostic" class="ar.edu.unq.desapp.groupb.model.Diagnostic"
			cascade="save-update" lazy="false"></one-to-one>-->

	</class>
</hibernate-mapping>
